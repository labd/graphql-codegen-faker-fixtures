{
  "name": "graphql-codegen-faker-fixtures",
  "description": "Generate faker fixtures for your fields",
  "author": "Lab Digital <opensource@labdigital.nl>",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@8.6.7",
  "scripts": {
    "build": "tsup",
    "example:ts": "graphql-codegen --config example/codegen.ts",
    "example:yml": "graphql-codegen --config example/codegen.yml",
    "format": "prettier \"**/*.{ts,tsx,md}\" --write",
    "lint": "eslint --max-warnings 0",
    "start": "tsup --watch",
    "test": "vitest --run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "deepmerge-ts": "5.1.0",
    "graphql": "^16.7.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@faker-js/faker": "^8.0.2",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/plugin-helpers": "^5.0.0",
    "@labdigital/eslint-config-node": "^0.1.0",
    "@types/node": "^20.4.2",
    "graphql-tag": "^2.12.6",
    "prettier": "^3.0.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  }
}
